<!-- 搜索栏 -->
<view class="search">
  <input placeholder="检查点名称/详细地址" type='text' confirm-type='search'></input>
  <image src="../../assets/check/search.png" />
  <view class="line"></view>
</view>
<view class="function">
  <view class="distance">距离排序</view>
</view>
<!-- 内容布局 -->
  <view class="card" wx:for="{{list}}" wx:key="key">
    <view class="title">{{item.pointName}}</view>
    <view class="area">检查项中的第三项填写有误，需要修改。</view>
    <view class="twoLine">
      <view class="currentTime">{{item.gmtCreate}}</view>
      <!-- <view class="modify">第3次修改</view> -->
    </view>
    <view class="buttons">
      <view class="button" bindtap="exAgree" data-examineid="{{item.id}}">同意</view>
      <view class="button" bindtap="exDisAgree" data-examineid="{{item.id}}">拒绝</view>
    </view>
    <view class="name">{{item.checkPersonId}}</view>
  </view>
